    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmux/koot.git"
  },
  "scripts": {
    "start": "koot-start",
    "start:server": "koot-start --no-build",
    "build": "koot-build",
    "build:client": "koot-build -c",
    "build:server": "koot-build -s",
    "build:spa": "koot-build --type spa",
    "build:qa": "koot-build -c --config koot.build.qa.js",
    "build:custom": "koot-build --config koot.build.custom-output.js",
    "dev": "koot-dev",
    "dev:client": "koot-dev -c",
    "dev:server": "koot-dev -s",
    "dev:global-pm2": "koot-dev -g",
    "dev:no-open": "koot-dev --no-open",
    "dev:spa": "koot-dev --type spa",
    "dev:qa": "koot-dev --config koot.build.qa.js",
    "analyze": "koot-analyze",
    "eslint": "eslint \"src/**/*.+(js|jsx)\""
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "koot": "^0.8.0-alpha.6",
    "koot-redux": "^1.0.9",
    "koot-router": "^1.0.5",
