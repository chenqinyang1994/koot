// See https://go.microsoft.com/fwlink/?LinkId=759670
// for the documentation about the jsconfig.json format
{
    "compilerOptions": {
        "target": "esnext",
        "module": "es6",
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "@src/*": ["./src/*"],
            "@app/*": ["./src/app/*"],
            "@ui/*": ["./src/app/ui/*"],
            "@api/*": ["./src/app/api/*"],
            "@redux/*": ["./src/app/redux/*"],
            "@libs/*": ["./src/libs/*"],
            "@assets/*": ["./src/assets/*"],
            "~Assets/*": ["./src/assets/*"]
        }
    },
    "exclude": [
        ".*",
        "dist",
        "dist*",
        "node_modules"
    ]
}
